{
  "name": "conversational-form",
  "version": "2.0.0",
  "description": "Turning web forms into conversations",
  "main": "dist/conversational-form.js",
  "directories": {
    "dist": "distribution"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/space10-community/conversational-form"
  },
  "author": {
    "name": "Felix Nielsen",
    "email": "felix.nielsen@rwatgg.dk",
    "url": "http://rwatgg.dk"
  },
  "contributors": [
    {
      "name": "Jens Soegaard",
      "email": "jens@jenssogaard.com",
      "url": "https://jenssogaard.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/space10-community/conversational-form/issues"
  },
  "homepage": "https://github.com/space10-community/conversational-form",
  "ignore": [
    "build"
  ],
  "scripts": {
    "bundle-fuzzy": "browserify src/scripts/cf/vendor/fast-fuzzy-entry.js -o build/cf/vendor/fast-fuzzy-standalone.js --standalone FastFuzzy",
    "build": "gulp build",
    "build:all": "npm run bundle-fuzzy && npm run build",
    "copy-dist": "sh scripts/copy-dist.sh",
    "build:copy": "npm run build:all && npm run copy-dist"
  },
  "dependencies": {
    "fast-fuzzy": "^1.12.0"
  },
  "devDependencies": {
    "browserify": "^17.0.1",
    "graceful-fs": "^4.2.11",
    "gulp": "^4.0.2",
    "jasmine-core": "^4.6.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-firefox-launcher": "^2.1.3",
    "karma-jasmine": "^5.1.0"
  }
}
